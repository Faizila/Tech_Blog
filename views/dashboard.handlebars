 
<div class="row align-center mb-5">
  <div class="col-md-6">
        <a href="/dashboard/create/"><button id="create" class="button">Create Post</button></a>
        
        <h3>Dashboard: your posts</h3><hr>
{{#if posts.length}}
    </div>
    </div>

    {{#each posts as |post|}}
        {{> postData post}}
        
    <a href="/dashboard/edit/{{post.id}}"><button class="button">Edit Post</button></a>
    </div>
    </div>
    {{/each}}

{{else}}
<h6>You currently have no posts!</h6>

{{/if}}